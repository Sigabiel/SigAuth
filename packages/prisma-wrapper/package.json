{
    "name": "@sigauth/prisma-wrapper",
    "version": "0.0.1",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc"
    },
    "exports": {
        "./prisma": {
            "import": "./dist/extended-prisma.js",
            "types": "./dist/extended-prisma.d.ts",
            "default": "./dist/extended-prisma.js"
        },
        "./prisma-client": {
            "import": "./prisma/generated/client/index.js",
            "types": "./prisma/generated/client/index.d.ts",
            "default": "./prisma/generated/client/index.js"
        },
        "./json-types": {
            "import": "./dist/json-types.js",
            "types": "./dist/json-types.d.ts",
            "default": "./dist/json-types.js"
        },
        "./protected": {
            "import": "./dist/protected.js",
            "types": "./dist/protected.d.ts",
            "default": "./dist/protected.js"
        }
    },
    "dependencies": {
        "@prisma/client": "^6.16.2"
    },
    "devDependencies": {
        "prisma": "^6.12.0",
        "typescript": "latest"
    }
}
